<div class="homepage_container">
  <div class="header">
    <%= link_to root_path do %>
      <%= image_tag("TopHouse_Title.png")  %>
    <% end %>
  </div>

  <div class="space_maker"></div>

  <div class="homepage_body container-fluid">
    <div class="row" style="text-align:center;">
      <%= link_to music_path, class: "pics col-lg-4" do %>
        <div class="imagediv">
          <div class="homepage_link_word">
            <h1>Music</h1>
          </div>
          <%= image_tag("TopHouse_Jesse.png", :class => "individual_pic")  %>
        </div>
      <% end %>


      <%= link_to shows_path, class: "pics col-lg-4" do  %>
        <div class="imagediv">
          <div class="homepage_link_word">
            <h1>Shows</h1>
          </div>
          <%= image_tag("TopHouse_Joe.png", :class => "individual_pic") %>
        </div>
      <% end %>

      <%= link_to about_path, class: "pics col-lg-4" do  %>
        <div class="imagediv">
          <div class="homepage_link_word">
          <h1>About</h1>
          </div>
          <%= image_tag("TopHouse_William.png", :class => "individual_pic")  %>
        </div>
      <% end %>
    </div>
  </div>

  <div class="space_maker"></div>
</div>
