<div class="homepage_container">
  <div class="header">
    <%= image_tag("TopHouse_Title.png") %>
  </div>
  <div class="animate_ele">
    <div class="space_maker"></div>

    <div class="homepage_body">

      <%= link_to music_path, class: "pics" do %>
      <div>
        <h1 class="homepage_link_word">Music</h1>
        <%= image_tag("TopHouse_Jesse.png") %>
      </div>
    <% end %>

    <%= link_to shows_path, class: "pics" do %>
    <div>
      <h1 class="homepage_link_word">Shows</h1>
      <%= image_tag("TopHouse_Joe.png") %>
    </div>
  <% end %>

  <%= link_to about_path, class: "pics" do %>
  <div>
    <h1 class="homepage_link_word">About</h1>
    <%= image_tag("TopHouse_William.png") %>
  </div>
<% end %>
</div>

<div class="space_maker"></div>
</div>
</div>
